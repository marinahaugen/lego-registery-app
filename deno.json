{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/",
    "test": "deno run -A --node-modules-dir npm:vitest --run --reporter=basic",
    "test:watch": "deno run -A --node-modules-dir npm:vitest --watch --reporter=verbose",
    "test:debug": "deno run -A --node-modules-dir npm:vitest --run --reporter=verbose",
    "test:ui": "deno run -A --node-modules-dir npm:vitest --ui",
    "test:coverage": "deno run -A --node-modules-dir npm:vitest --coverage"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react",
    "types": ["./src/vite-env.d.ts"]
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@types/react": "npm:@types/react@^19.0.10",
    "@types/react-dom": "npm:@types/react-dom@^19.0.4",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "jsdom": "npm:jsdom@^26.1.0",
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^19.0.0",
    "vite": "npm:vite@^6.1.1",
    "@supabase/supabase-js": "npm:@supabase/supabase-js@^2.53.0",
    "zod": "npm:zod@^4.0.0/v4",
    "@mui/material": "npm:@mui/material@^7.2.0",
    "@emotion/react": "npm:@emotion/react@^11.13.1",
    "@emotion/styled": "npm:@emotion/styled@^11.13.1",
    "vitest": "npm:vitest@^2.1.8",
    "@testing-library/react": "npm:@testing-library/react@^16.1.0",
    "@testing-library/user-event": "npm:@testing-library/user-event@^14.5.2",
    "@testing-library/jest-dom": "npm:@testing-library/jest-dom@^6.6.3",
    "swr": "npm:swr@^2.2.5"
  }
}
